#include <bits/stdc++.h>
using namespace std;

struct Node {
int data;
Node* next;
}

void Print (Node* head) {
if (head == NULL) //exit condition for recursion
{
return;
}

cout<<head->data<<endl;  //First print the data
Print(head->next);       //recursive call;
}


void ReversePrint (Node* head)
{
if (head == NULL)
{
return;
}
ReversePrint(head->next);  
cout<<head->data<<" ";
}






int main()
{
Node* head = NULL;
.
.
.}
